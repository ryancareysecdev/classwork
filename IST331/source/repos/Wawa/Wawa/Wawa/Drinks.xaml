<Window x:Class="Wawa.Drinks"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wawa"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnBottled" HorizontalAlignment="Left" Margin="66,148,0,0" VerticalAlignment="Top" Width="163" Height="134" Click="BtnBottled_Click">
            <Image Source="img_bottled_2018.png" Stretch="Fill" Height="124" Width="173"/>
        </Button>
        <Button x:Name="btnCoffee" HorizontalAlignment="Left" Margin="558,145,0,0" VerticalAlignment="Top" Width="169" Height="134" Click="BtnCoffee_Click">
            <Image Source="img_coffee_2020.png" Stretch="Fill" Height="129" Width="193"/>
        </Button>
        <Button x:Name="btnFrozen" HorizontalAlignment="Left" Margin="324,139,0,0" VerticalAlignment="Top" Width="161" Height="150" BorderBrush="#FFF7F2F2" Click="BtnFrozen_Click">
            <Image Source="img_frozen_2020.png" Stretch="Fill" Height="119" Width="151"/>
        </Button>
        <Button x:Name="btnReturntoMain" Content="Back to main" HorizontalAlignment="Left" Margin="66,342,0,0" VerticalAlignment="Top" Width="135" Height="38" Click="BtnReturntoMain_Click"/>
    </Grid>
</Window>
